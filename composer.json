{
    "name": "juampi92/cursor-pagination",
    "description": "Cursor pagination made easy for Laravel",
    "homepage": "https://github.com/juampi92/cursor-pagination",
    "license": "MIT",
    "keywords": ["laravel", "pagination"],
    "authors": [
        {
            "name": "juampi92",
            "email": "juampi92@gmail.com"
        }
    ],
    "require": {
      "php": ">=7.0.0",
      "illuminate/support": "5.5.*||5.6.*",
      "illuminate/http": "5.5.*||5.6.*",
      "illuminate/database": "5.5.*||5.6.*"
    },
    "require-dev": {
      "orchestra/testbench": "~3.5",
      "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Juampi92\\CursorPagination\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juampi92\\CursorPagination\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juampi92\\CursorPagination\\CursorPaginationServiceProvider"
            ],
            "aliases": {
                "CursorPagination": "Juampi92\\CursorPagination\\Facades\\CursorPagination"
            }
        }
    }
}
